EVENT ProtectiveBondSetHP
VARS
	CHARACTER:_Character
	FLOAT:_HP
	STRING: _Text
	FIXEDSTRING: _Disp
ON
	OnCharacterStatusApplied(_Character, "PROTECTIVE_BOND_DAMAGE")
ACTIONS
	IF	"c1"
		CharacterGetStat(_HP, _Character, VitalityPoints)
	THEN
		Set(%ProtectiveBondHP, _HP)
	ENDIF

EVENT ProtectiveBondChangeinHP
VARS
	CHARACTER:_Character
	CHARACTER:_Source
	FLOAT:_HP
	FLOAT:_MaxHP
	FLOAT:_CalculatedHP
	FLOAT:_SourceNewHP
	FLOAT:_SourceHP
	FLOAT:_SourceMaxHP
	FLOAT:_CalculatedSourceHP
	FLOAT3:_Pos
	STRING: _Text
	FIXEDSTRING: _Disp
ON
	OnCharacterVitalityChanged(_Character,_)
ACTIONS
	IF	"c1&c2"
		CharacterHasStatus(_Character,PROTECTIVE_BOND_DAMAGE)
		CharacterGetStatusSourceCharacter(_Character,PROTECTIVE_BOND_DAMAGE,_Source)
	THEN
		IF "c1&(c2|c3)"
			CharacterGetStat(_HP, _Character, VitalityPoints)
			IsGreaterThen(_HP, %ProtectiveBondHP)
			IsEqual(_HP, %ProtectiveBondHP)
		THEN
			Set(%ProtectiveBondHP, _HP)
			CharacterRemoveStatus(_Character,PROTECTIVE_BOND_DAMAGE)
		ELIF "c1&c2&c3&c4&c5"
			IsLessThen(_HP, %ProtectiveBondHP)
			CharacterGetStat(_HP, _Character, VitalityPoints)
			CharacterGetStat(_MaxHP, _Character, VitalityMax)
			CharacterGetStat(_SourceHP, _Source, VitalityPoints)
			CharacterGetStat(_SourceMaxHP, _Source, VitalityMax)
		THEN
			Set(_CalculatedHP, %ProtectiveBondHP)
			Subtract(_CalculatedHP, _HP)
			Add(_CalculatedHP,_HP)
			//Cast(_Text, _CalculatedHP)
			//Cast(_Disp, _Text)
			//StatusText(_Character, _Disp)
			Divide(_CalculatedHP,_MaxHP)
			SetHealth(_Character,_CalculatedHP)
			Multiply(_CalculatedHP,_MaxHP)
			Subtract(_CalculatedHP,_HP)
			Subtract(_SourceHP,_CalculatedHP)
			Divide(_SourceHP,_SourceMaxHP)
			SetHealth(_Source,_SourceHP)
			CharacterRemoveStatus(_Character,PROTECTIVE_BOND_DAMAGE)
		ENDIF
	ENDIF

