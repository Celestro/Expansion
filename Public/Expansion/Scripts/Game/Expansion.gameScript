	ItemGetStatusSourceCharacter(_Item,BOOK_OF_ANCIENT_SECRETS_RITUAL,_Character)
	CharacterHasStatus(_Character,SILENCE_RITUAL_PROC)
	ItemGetTemplate(_Item,_Template)
	IsEqual(_Template,50dea81e-23cc-49f3-92f8-ed3267e553b0)
