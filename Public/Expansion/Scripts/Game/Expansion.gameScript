EVENT DivineIntervention
VARS
CHARACTER:_Character
FLOAT:_Level
INT:_Result
STRING: _Text
FIXEDSTRING: _Roll

ON
	OnCharacterStatusApplied(_Character,DIVINE_INTERVENTION_TECHNICAL)
ACTIONS
	IF	"c1"
		CharacterGetStat(_Level,_Character,Level)
	THEN
		GetRandomBetween(_Result,1,100)
		IF "(c1|c2)"
			IsEqual(_Result,_Level)
			IsLessThen(_Result,_Level)
		THEN
			CharacterApplyStatus(_Character,DIVINE_INTERVENTION_TECHNICAL,-1)
			StatusText(_Character, "Divine Intervention")
		ELIF "!c1&!c2"
			IsEqual(_Result,_Level)
			IsLessThen(_Result,_Level)
		THEN
			Print(_Text, "Rolled a [1]", _Result)
			Cast(_Roll, _Text)
			StatusText(_Character, _Roll)
		ENDIF
	ENDIF

