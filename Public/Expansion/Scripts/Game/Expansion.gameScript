EVENT ClockworkCavalcade_Destroy
VARS
	CHARACTER:_Character
	ITEM:_Item
	CHARACTERTEMPLATE:_Template
	ITEMTEMPLATE:_ItemTemplate
ON
	OnCharacterStatusApplied(_Character, CLOCKWORK_CAVALCADE)
	OnItemStatus(_Item, CLOCKWORK_CAVALCADE)
ACTIONS
IF	"c1&c2"
	CharacterGetTemplate(_Character, _Template)
	IsEqual(_Template,"eddf2b83-21d3-4d6f-b958-c30f00dbbb92")
THEN
	CharacterDestroy(_Character)
ELIF	"c1&c2"
	ItemGetTemplate(_Character, _ItemTemplate)
	IsEqual(_Template,"63a52705-056b-4736-91ee-66b839de2afe")
THEN
	ItemDestroy(_Item)
ENDIF

EVENT RemoveAngelicForm
VARS
	CHARACTER:_Character
	ITEM:_Item
ON
	OnCharacterStatusRemoved(_Character,ANGELIC_FORM)
ACTIONS
IF	"c1&c2&c3"
	ItemGetFromInventory(_Item, _Character , "DivineSoul_AngelicForm")
	ItemGetTemplate(_Item, _Template)
	IsEqual(_Template,"ae9bdab1-10e5-4761-b767-c28f8319f3c7")
THEN
	CharacterRemoveFromInventory(_Character, "DivineSoul_AngelicForm")
	ItemDestroy(_Item)
ENDIF

